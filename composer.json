{
    "name": "WeImport-Backend",
    "description": "A slim application for we import backend",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdull-Rahman Al Haj Hassan",
            "email": "info@revision-lb.com",
            "homepage": "http://www.revision-lb.com/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "firebase/php-jwt": "^5.2",
        "monolog/monolog": "^1.17",
        "phpmailer/phpmailer": "^6.3",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "tuupola/base62": "^1.0",
        "tuupola/slim-jwt-auth": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
